<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <servlet-name>AddToShoppingCarServlet</servlet-name>
    <servlet-class>web.AddToShoppingCarServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ChangeNumberServlet</servlet-name>
    <servlet-class>web.ChangeNumberServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DeleteMerchandiseItemServlet</servlet-name>
    <servlet-class>web.DeleteMerchandiseItemServlet</servlet-class>
  </servlet>



  <servlet-mapping>
    <servlet-name>AddToShoppingCarServlet</servlet-name>
    <url-pattern>/addToShoppingCarAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ChangeNumberServlet</servlet-name>
    <url-pattern>/changeNumberAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteMerchandiseItemServlet</servlet-name>
    <url-pattern>/deleteMerchandiseItemAction</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
